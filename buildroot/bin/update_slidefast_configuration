#!/bin/bash
opt_set TEMP_SENSOR_BED 1
opt_enable PID_AUTOTUNE_MENU
opt_disable USE_ZMIN_PLUG
opt_enable USE_ZMAX_PLUG
opt_set X_MIN_ENDSTOP_INVERTING "true"
opt_set Y_MIN_ENDSTOP_INVERTING "true"
opt_set Z_MIN_ENDSTOP_INVERTING "true"
opt_set Z_MAX_ENDSTOP_INVERTING "true"
opt_set DEFAULT_AXIS_STEPS_PER_UNIT "{ 80, 80, 396.1, 98.90 }"
opt_set DEFAULT_MAX_FEEDRATE "{ 500, 500, 9, 25 }"
opt_set DEFAULT_MAX_ACCELERATION "{800,800,200,800}"
opt_set DEFAULT_ACCELERATION 500
opt_set DEFAULT_TRAVEL_ACCELERATION 500
opt_set DEFAULT_XJERK "20.0"
opt_set DEFAULT_YJERK "20.0"
opt_set DEFAULT_ZJERK "0.4"
opt_set INVERT_E0_DIR "true"
opt_set Z_HOME_DIR 1
opt_set Z_MAX_POS "193.28"
opt_set X_PROBE_OFFSET_FROM_EXTRUDER 0
opt_set Y_PROBE_OFFSET_FROM_EXTRUDER 0
opt_set HOMING_FEEDRATE_Z "(350)"
opt_set EXTRUDE_MAXLENGTH 1000
opt_set GRID_MAX_POINTS_X 2
opt_enable MESH_BED_LEVELING
opt_enable LCD_BED_LEVELING
opt_enable EEPROM_SETTINGS
opt_enable MESH_EDIT_MENU
opt_enable SOFT_ENDSTOPS_MENU_ITEM
opt_enable PRINTCOUNTER
opt_enable PIDTEMPBED
opt_set PREHEAT_1_TEMP_HOTEND 200
opt_set PREHEAT_1_TEMP_BED 50
opt_set DEFAULT_bedKp "405.67"
opt_set DEFAULT_bedKi "31.95"
opt_set DEFAULT_bedKd "1287.74"
opt_enable NOZZLE_PARK_FEATURE
opt_enable SDSUPPORT
opt_enable INDIVIDUAL_AXIS_HOMING_MENU
opt_enable REPRAP_DISCOUNT_SMART_CONTROLLER
opt_enable SPEAKER
#opt_enable FIX_MOUNTED_PROBE
opt_disable LCD_FEEDBACK_FREQUENCY_DURATION_MS
opt_disable LCD_FEEDBACK_FREQUENCY_HZ
opt_set THERMAL_PROTECTION_PERIOD 60
opt_set THERMAL_PROTECTION_HYSTERESIS 20
opt_set WATCH_TEMP_PERIOD 40
opt_enable EMERGENCY_PARSER
opt_enable ADVANCED_PAUSE_FEATURE
opt_set FILAMENT_CHANGE_SLOW_LOAD_LENGTH 50
opt_set FILAMENT_CHANGE_UNLOAD_LENGTH 740
opt_set FILAMENT_CHANGE_FAST_LOAD_LENGTH 760
opt_set FILAMENT_UNLOAD_RETRACT_LENGTH 0
opt_set FILAMENT_UNLOAD_DELAY 500
opt_set FILAMENT_UNLOAD_PURGE_LENGTH 0
opt_set FILAMENT_CHANGE_UNLOAD_FEEDRATE 60
opt_set FILAMENT_CHANGE_UNLOAD_ACCEL 15
opt_set FILAMENT_CHANGE_FAST_LOAD_FEEDRATE 60
opt_set FILAMENT_CHANGE_FAST_LOAD_ACCEL 15
opt_set THERMAL_PROTECTION_BED_HYSTERESIS 15
opt_set THERMAL_PROTECTION_BED_PERIOD 120
opt_set WATCH_BED_TEMP_PERIOD 120
opt_enable X_DRIVER_TYPE
opt_enable Y_DRIVER_TYPE
opt_enable Z_DRIVER_TYPE
opt_enable E0_DRIVER_TYPE
opt_set X_DRIVER_TYPE "TMC2100"
opt_set Y_DRIVER_TYPE "TMC2100"
opt_set Z_DRIVER_TYPE "TMC2100"
opt_set E0_DRIVER_TYPE "TMC2208_STANDALONE"

cp Marlin/Configuration* Marlin/src/config/examples/SlideFast/
restore_configs