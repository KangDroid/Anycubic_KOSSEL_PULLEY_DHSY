#!/bin/bash
use_example_configs delta/generic
opt_set DEFAULT_NOMINAL_FILAMENT_DIA 1.75
opt_set TEMP_SENSOR_0 5
opt_enable PID_AUTOTUNE_MENU
opt_set EXTRUDE_MAXLENGTH 500
opt_enable DELTA_HOME_TO_SAFE_ZONE
opt_set DELTA_CALIBRATION_DEFAULT_POINTS 4
opt_set DELTA_CALIBRATION_RADIUS 80
opt_set PROBE_MANUALLY_STEP "0.025"
opt_set DELTA_PRINTABLE_RADIUS "90.0"
opt_set DELTA_DIAGONAL_ROD "217.0"
opt_set DELTA_HEIGHT "339.25"
opt_set DELTA_RADIUS "90.0"
opt_set X_MIN_ENDSTOP_INVERTING "false"
opt_set Y_MIN_ENDSTOP_INVERTING "false"
opt_set X_MAX_ENDSTOP_INVERTING "false"
opt_set Y_MAX_ENDSTOP_INVERTING "false"
opt_set Z_MAX_ENDSTOP_INVERTING "false"
opt_set DEFAULT_AXIS_STEPS_PER_UNIT "{ DEFAULT_XYZ_STEPS_PER_UNIT, DEFAULT_XYZ_STEPS_PER_UNIT, DEFAULT_XYZ_STEPS_PER_UNIT, 96 }"
opt_set DEFAULT_MAX_FEEDRATE "{200, 200, 200, 200}"
opt_set DEFAULT_MAX_ACCELERATION "{3000,3000,3000,3000}"
opt_enable FIX_MOUNTED_PROBE
opt_enable DELTA_AUTO_CALIBRATION
opt_add DHSY_KOSSEL_FAN
opt_set DELTA_CALIBRATION_DEFAULT_POINTS 5
opt_set X_PROBE_OFFSET_FROM_EXTRUDER 0
opt_set Y_PROBE_OFFSET_FROM_EXTRUDER 0
opt_set Z_PROBE_OFFSET_FROM_EXTRUDER "-19.6"
opt_set Z_PROBE_OFFSET_RANGE_MAX 60
opt_set Z_PROBE_OFFSET_RANGE_MIN -60
opt_set INVERT_X_DIR "true"
opt_set INVERT_Y_DIR "true"
opt_set INVERT_Z_DIR "true"
opt_set INVERT_E0_DIR "true"
opt_enable SOFT_ENDSTOPS_MENU_ITEM
opt_enable EEPROM_SETTINGS
opt_set PREHEAT_1_TEMP_HOTEND 200
opt_set PREHEAT_1_TEMP_BED 50
opt_enable NOZZLE_PARK_FEATURE
opt_enable SDSUPPORT
opt_enable REPRAP_DISCOUNT_SMART_CONTROLLER
opt_set_adv Z_HOME_BUMP_MM 5
opt_set_adv HOMING_BUMP_DIVISOR "{ 10, 10, 10 }"
opt_set_adv MANUAL_FEEDRATE "{50*60, 50*60, 50*60, 60}"
opt_disable_adv SLOWDOWN
opt_enable EMERGENCY_PARSER
cp Marlin/Configuration* Marlin/src/config/examples/Kossel/
restore_configs