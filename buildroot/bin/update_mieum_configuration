#!/bin/bash
restore_configs

opt_set DEFAULT_NOMINAL_FILAMENT_DIA "1.75"
opt_set TEMP_SENSOR_BED 1
opt_enable PID_AUTOTUNE_MENU
opt_set DEFAULT_Kp "27.48"
opt_set DEFAULT_Ki "2.33"
opt_set DEFAULT_Kd "81.03"
opt_set EXTRUDE_MAXLENGTH "800"
opt_enable COREXY
opt_set X_MIN_ENDSTOP_INVERTING "true"
opt_set Y_MIN_ENDSTOP_INVERTING "true"
opt_set DEFAULT_AXIS_STEPS_PER_UNIT "{ 50, 50, 250, 73.5 }"
opt_set DEFAULT_MAX_FEEDRATE "{ 9000, 9000, 1000, 1000 }"
opt_set DEFAULT_MAX_ACCELERATION "{ 800, 800, 200, 1000 }"
opt_set DEFAULT_ACCELERATION "500"
opt_set DEFAULT_RETRACT_ACCELERATION "1500"
opt_set DEFAULT_TRAVEL_ACCELERATION "500"
opt_enable FIX_MOUNTED_PROBE
opt_set NOZZLE_TO_PROBE_OFFSET "{0, -31, -1.4}"
opt_set INVERT_X_DIR "true"
opt_set INVERT_E0_DIR "true"
opt_set Z_MAX_POS "170"
opt_enable SOFT_ENDSTOPS_MENU_ITEM
#opt_enable FILAMENT_RUNOUT_SENSOR
#opt_enable Z_SAFE_HOMING
opt_enable NOZZLE_PARK_FEATURE
opt_set HOMING_FEEDRATE_Z "(500)"
opt_enable EEPROM_SETTINGS
opt_set PREHEAT_1_TEMP_HOTEND "190"
opt_set PREHEAT_1_TEMP_BED "65"
opt_enable SDSUPPORT
opt_enable INDIVIDUAL_AXIS_HOMING_MENU
opt_enable REPRAP_DISCOUNT_SMART_CONTROLLER
opt_enable X_DRIVER_TYPE
opt_enable Y_DRIVER_TYPE
opt_enable Z_DRIVER_TYPE
opt_enable E0_DRIVER_TYPE
opt_set X_DRIVER_TYPE "TMC2208_STANDALONE"
opt_set Y_DRIVER_TYPE "TMC2208_STANDALONE"
opt_set Z_DRIVER_TYPE "TMC2208_STANDALONE"
opt_set E0_DRIVER_TYPE "TMC2208_STANDALONE"
#opt_set SD_FINISHED_RELEASECOMMAND '"G1 F1000 Z170"'
opt_set THERMAL_PROTECTION_PERIOD 60
opt_set THERMAL_PROTECTION_HYSTERESIS 20
opt_set WATCH_TEMP_PERIOD 40
opt_enable EMERGENCY_PARSER
opt_enable ADVANCED_PAUSE_FEATURE
opt_set FILAMENT_CHANGE_SLOW_LOAD_LENGTH 50
opt_set FILAMENT_CHANGE_UNLOAD_LENGTH 740
opt_set FILAMENT_CHANGE_FAST_LOAD_LENGTH 700
opt_set FILAMENT_UNLOAD_PURGE_RETRACT 0
opt_set FILAMENT_UNLOAD_PURGE_DELAY 100
opt_set FILAMENT_UNLOAD_PURGE_LENGTH 0
opt_set FILAMENT_CHANGE_UNLOAD_FEEDRATE 80
opt_set FILAMENT_CHANGE_UNLOAD_ACCEL 20
opt_set FILAMENT_CHANGE_FAST_LOAD_FEEDRATE 80
opt_set FILAMENT_CHANGE_FAST_LOAD_ACCEL 20
opt_set THERMAL_PROTECTION_BED_HYSTERESIS 15
opt_set THERMAL_PROTECTION_BED_PERIOD 120
opt_set WATCH_BED_TEMP_PERIOD 120

rm -rf config/examples/Mieum/*
cp Marlin/Configuration* config/examples/Mieum/
restore_configs