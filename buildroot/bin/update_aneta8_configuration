#!/bin/sh
opt_set TEMP_SENSOR_0 5
opt_set TEMP_SENSOR_BED 5
opt_set X_MIN_ENDSTOP_INVERTING "true"
opt_set Y_MIN_ENDSTOP_INVERTING "true"
opt_set Z_MIN_ENDSTOP_INVERTING "true"
opt_set DEFAULT_AXIS_STEPS_PER_UNIT "{ 50, 50, 200, 50 }"
opt_set DEFAULT_MAX_FEEDRATE "{ 400, 400, 8, 50 }"
opt_set DEFAULT_MAX_ACCELERATION "{ 1000, 1000, 100, 10000 }"
opt_set DEFAULT_ACCELERATION 400
opt_set DEFAULT_RETRACT_ACCELERATION 1000
opt_set DEFAULT_TRAVEL_ACCELERATION 100
opt_set X_PROBE_OFFSET_FROM_EXTRUDER 0
opt_set Y_PROBE_OFFSET_FROM_EXTRUDER 0
opt_set XY_PROBE_SPEED 6000
opt_set INVERT_Y_DIR "true"
opt_set INVERT_Z_DIR "false"
opt_set INVERT_X_DIR "true"
opt_set INVERT_E0_DIR "true"
opt_set X_BED_SIZE "220"
opt_set Y_BED_SIZE "220"
opt_set Z_MAX_POS 340
opt_set X_DRIVER_TYPE "TMC2208_STANDALONE"
opt_set Y_DRIVER_TYPE "TMC2208_STANDALONE"
opt_set Z_DRIVER_TYPE "TMC2208_STANDALONE"
opt_set E0_DRIVER_TYPE "TMC2208_STANDALONE"
opt_enable EEPROM_SETTINGS
opt_enable INDIVIDUAL_AXIS_HOMING_MENU
opt_set PREHEAT_1_TEMP_HOTEND 200
opt_set Y_MIN_POS "-16"
opt_set PREHEAT_1_TEMP_BED 50
opt_enable MESH_EDIT_MENU
opt_enable PRINTCOUNTER
opt_enable MESH_BED_LEVELING
opt_enable LCD_BED_LEVELING
opt_set GRID_MAX_POINTS_X 2
opt_enable SDSUPPORT
opt_enable REPRAP_DISCOUNT_SMART_CONTROLLER
opt_set HOMING_FEEDRATE_XY "(100*60)"
opt_enable Z_DUAL_STEPPER_DRIVERS
cp Marlin/Configuration* config/examples/AnetA8/
restore_configs